{
  "data": {
    "ticker": "BTC-USD",
    "period": "10y",
    "interval": "1d",
    "train_split_size": 0.80
    },
    "model": {
        "input_dim": 2,
        "hidden_dim": 128,
        "layer_dim": 1,
        "output_dim": 1,
        "dropout_prob": 0.2
    },
    "training": {
        "batch_size": 5,
        "n_epochs": 50,
        "learning_rate": 1e-4,
        "weight_decay":1e-6
    },
    "loss":{
        "alpha" : 0.4,
        "gamma" : 0.2
    }
}